{
  "name": "njahit",
  "description": "A stitcher",
  "version": "1.0.0",
  "private": true,
  "author": "Rama Rahmanda",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "1.6.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@monaco-editor/react": "4.2.1",
    "@types/js-cookie": "2.2.7",
    "@types/js-yaml": "4.0.2",
    "@types/jsonfile": "6.0.1",
    "@types/uuid": "8.3.1",
    "ajv": "8.6.2",
    "axios": "0.21.1",
    "framer-motion": "^4",
    "handlebars": "4.7.7",
    "immer": "9.0.5",
    "js-cookie": "2.2.1",
    "js-yaml": "4.1.0",
    "json-schema": "0.3.0",
    "jsonfile": "6.1.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flow-renderer": "9.6.5",
    "react-icons": "4.2.0",
    "swr": "0.5.6",
    "uuid": "8.3.2",
    "vm": "0.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/node": "14.17.5",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.30.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
